
#include<stdio.h>
#include <math.h>
int main()
 
{
 
float a;
float b;
float c;
float d;
float x;
float ris;

 

printf("risoluzione di -ax^3 +bx^2 - c^x + d \n");

printf("inserisci a");
scanf("%f",&a);
printf("inserisci b");
scanf("%f",&b);
printf("inserisci c");
scanf("%f",&c);
printf("inserisci d");
scanf("%f",&d);
printf("inserisci x");
scanf("%f",&x);

ris=-a*pow(x,3)+b*pow(x,2)-c*x+d;
printf("visualizza il risultato %d",ris);
return 0;
}
 

